
<div class = 'container'>
  <%= simple_form_for @itineray do |f| %>
  <%= f.input :name %>
  <%= f.input :start_date, as: :date, html5: true, required: true %>
  <%= f.input :end_date, as: :date, html5: true, required: true %>
  <%= f.input :country %>
  <%= f.button :submit, class: 'btn btn-primary' %>
  <%end%>
</div>
